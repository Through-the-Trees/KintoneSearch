{
    "manifest_version": 1,
    "version": "0.0.1",
    "type": "APP",
    "name": {
      "en": "Custom Search Plugin (IN DEVELOPMENT)"
    },
    "description": {
      "en": "Adds a custom search bar to kintone apps that utilizes custom sql queries to simplify searches"
    },
    "icon": "image/check.png",
    "homepage_url": {
      "en": "https://github.com/Through-the-Trees/KintoneSearch"
    },
    "desktop": {
      "js": [
        "lib/kuc.min.js",
        "lib/vue.global.min.js",
        "js/desktop.js"
      ]
    },
    "config": {
    "html": "html/config.html",
    "js": [
      "lib/kintone-config-helper.js",
      "js/config.js"
    ],
    "css": [
      "css/51-modern-default.css",
      "css/config.css"
    ],
    "required_params": ["searchFields"]
    }
  }